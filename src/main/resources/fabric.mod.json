{
	"schemaVersion": 1,
	"id": "${id}",
	"name": "${display_name}",
	"version": "${modversion}",
	"description": "${description}",
	"authors": [
		"${author}"
	],
	"contact": {
		"sources": "https://github.com/${github}/OffHandTickFix",
		"issues": "https://github.com/${github}/OffHandTickFix/issues"
	},
	"license": "${license}",
	"icon": "assets/${id}/logo.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"${group}.${namespace}.${name}"
		],
		"client": [
			"${group}.${namespace}.${name}"
		]
	},
	"mixins": [
		"${id}.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": "${mc}",
		"fabric-api": "*",
		"necronomicon": ">=1.6.0"
	}
}